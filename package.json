{
  "author": "Alex Potsides",
  "name": "mongoose-crate",
  "description": "Attach files to MongoDB models via Mongoose.js",
  "license": "ISC",
  "keywords": [
    "mongoose",
    "s3",
    "imagemagick",
    "uploads",
    "attachments",
    "files"
  ],
  "version": "1.1.0",
  "homepage": "https://github.com/achingbrain/mongoose-crate",
  "repository": {
    "type": "git",
    "url": "git://github.com/achingbrain/mongoose-crate.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "mocha --recursive",
    "test:coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- --recursive",
    "test:coverage:check": "istanbul cover _mocha -- --recursive && istanbul check-coverage --statement 100 --branch 100 --function 100",
    "lint": "standard",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "async": "^2.1.4",
    "check-types": "^7.0.0",
    "mmmagic": "^0.4.4"
  },
  "devDependencies": {
    "coveralls": "^2.8",
    "istanbul": "^0.4.3",
    "mocha": "^3.2.0",
    "mockgoose": "^6.0.8",
    "mongoose": "^4.7.0",
    "pre-commit": "^1.0.6",
    "should": "^11.1.1",
    "sinon": "^1.9",
    "standard": "^8.6.0"
  },
  "pre-commit": [
    "test:coverage:check",
    "lint"
  ]
}
